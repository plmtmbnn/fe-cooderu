{
  "extends": [
    "next",
    "next/core-web-vitals"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "react/no-unescaped-entities": "off",
    "@next/next/no-page-custom-font": "off",
    "react-hooks/exhaustive-deps": "error",
    "react/require-default-props": "off",
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ],
    "radix": [
      0
    ],
    "import/no-extraneous-dependencies": [
      0
    ]
  }
}